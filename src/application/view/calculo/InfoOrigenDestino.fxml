                            <TableColumn fx:id="fincaColumn" prefWidth="230.0" text="Finca" />
                              <TableColumn fx:id="distanciaColumn" prefWidth="139.0" text="Distancia" />
                          </columns>
                        </TableView>
                        <HBox alignment="CENTER" prefHeight="45.0" prefWidth="600.0" spacing="10.0">
